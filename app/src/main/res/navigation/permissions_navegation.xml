<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/permissions_navegation"
    app:startDestination="@id/Localization_Fragment">



    <fragment
        android:id="@+id/Localization_Fragment"
        android:name="com.cybergod.oyeetaxi.ui.permissions.fragments.LocalizationFragment"
        tools:layout="@layout/fragment_permission_localization">
        <action
            android:id="@+id/action_Localization_Fragment_to_Localization_Background_Fragment2"
            app:destination="@id/Localization_Background_Fragment" />
        <action
            android:id="@+id/action_Localization_Fragment_to_Storage_Fragment2"
            app:destination="@id/Storage_Fragment" />
    </fragment>

    <fragment
        android:id="@+id/Localization_Background_Fragment"
        android:name="com.cybergod.oyeetaxi.ui.permissions.fragments.LocalizationBackgroundFragment"
        tools:layout="@layout/fragment_permission_localization" >
        <action
            android:id="@+id/action_Localization_Background_Fragment_to_Storage_Fragment3"
            app:destination="@id/Storage_Fragment" />
    </fragment>


    <fragment
        android:id="@+id/Storage_Fragment"
        android:name="com.cybergod.oyeetaxi.ui.permissions.fragments.StorageFragment"
        tools:layout="@layout/fragment_permission_storage" />
</navigation>